{
  "name": "Temp warning",
  "state": "ACTIVE",
  "weight": 0,
  "pluginToken": "SH_controller",
  "filters": [
    {
      "configuration": {
        "filter": "typeof temperature !== 'undefined'"
      },
      "clazz": "org.thingsboard.server.extensions.core.filter.DeviceTelemetryFilter",
      "name": "tel"
    }
  ],
  "processor": {
    "configuration": {
      "alarmPropagateFlag": true,
      "alarmDetailsTemplate": "{\"sensor exception\":}",
      "newAlarmExpression": "temperature < 22 || temperature > 25",
      "alarmSeverity": "MINOR",
      "alarmStatus": "ACTIVE_UNACK",
      "alarmTypeTemplate": "temp",
      "clearAlarmExpression": "temperature >= 22 && temperature <= 25 "
    },
    "clazz": "org.thingsboard.server.extensions.core.processor.AlarmProcessor",
    "name": "temp"
  },
  "action": {
    "configuration": {
      "sync": true,
      "actionPath": "action",
      "requestMethod": "POST",
      "expectedResultCode": 200,
      "template": "test"
    },
    "clazz": "org.thingsboard.server.extensions.rest.action.RestApiCallPluginAction",
    "name": "test"
  },
  "additionalInfo": null
}